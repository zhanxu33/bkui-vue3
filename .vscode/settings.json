{
  
  "eslint.format.enable": true,
  "stylelint.validate": ["css", "less", "postcss", "scss", "vue"],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.arrowParens": "avoid",
  "prettier.bracketSameLine": false,
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 120,
  "prettier.quoteProps": "as-needed",
  "prettier.semi": true,
  "prettier.singleAttributePerLine": true,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "all",
  "prettier.useTabs": false,
  "prettier.vueIndentScriptAndStyle": true,
  "preCI.localCodeCheck.filter.whitePath": "",
  "preCI.localCodeCheck.filter.skipPath": ".*/\\.codecc/.*",
  "preCI.localCodeCheck.preCommit": "none",
  "preCI.localCodeCheck.prePush": "none",
  "preCI.pipeline.yamlFilePath": "build.yml",
  "preCI.localCodeCheck.toolType": "binary",
  "preCI.localCodeCheck.checkerSet.Other": [
    "standard_scc"
  ],
  "preCI.localCodeCheck.checkerSet.JS": [
    "standard_js"
  ],
  "preCI.localCodeCheck.checkerSet.TS": [
    "ts_standard"
  ],
  "i18n-ally.localesPaths": [
    "lib/locale",
    "packages/locale",
    "lib/locale/lang",
    "packages/locale/lang",
    "packages/locale/src/lang"
  ],
  "bk-code-ai.enable": false
}
